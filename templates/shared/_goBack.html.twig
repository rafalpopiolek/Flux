<a
        href="{% if route is defined %} {{ route }} {% else %} {{ path('app_home') }} {% endif %}"
        data-controller="popover"
        data-action="mouseenter->popover#show mouseleave->popover#hide"
        class="relative"
>
    <svg xmlns="http://www.w3.org/2000/svg" height="20" width="20" viewBox="0 0 512 512"
         class="fill-indigo-600 cursor-pointer">
        <path d="M9.4 233.4c-12.5 12.5-12.5 32.8 0 45.3l128 128c12.5 12.5 32.8 12.5 45.3 0s12.5-32.8 0-45.3L109.3 288 480 288c17.7 0 32-14.3 32-32s-14.3-32-32-32l-370.7 0 73.4-73.4c12.5-12.5 12.5-32.8 0-45.3s-32.8-12.5-45.3 0l-128 128z"></path>
    </svg>

    <template data-popover-target="content">
        <div
                data-popover-target="card"
                class="rounded shadow-lg absolute left-0 bottom-6 z-50 bg-gray-200 dark:bg-gray-600 dark:text-gray-300"
        >
            <p class="py-1 px-2">Go back</p>
        </div>
    </template>
</a>